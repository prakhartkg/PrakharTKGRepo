package com.yash.controler;

import java.io.IOException;

import javax.servlet.ServletException;
import javax.servlet.http.HttpServletRequest;
import javax.servlet.http.HttpServletResponse;

import com.yash.domain.Customer;



public class HelperControler {
	public void doProcess(HttpServletRequest request,HttpServletResponse response,Customer customer) throws ServletException, IOException{
		String url=(String) request.getAttribute("action");
		request.getRequestDispatcher(url).forward(request, response);;
	}
}
